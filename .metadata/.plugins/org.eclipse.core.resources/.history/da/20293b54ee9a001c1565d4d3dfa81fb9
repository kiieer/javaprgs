package com.revature.java.store;
import com.revature.java.store.Customer;

public class Create {
	public static void main(String[] args) {
		Customer cus1 = new Customer("Kiersten", 20.00, 239.23, 4);
		System.out.println(cus1.toString());
		
		Customer cus2 = new Customer("Eric", 105.00, 30.54, 4);
		System.out.println(cus2.toString());
		
		Customer cus3 = new Customer("Shital", 6.26, 20.36, 4);
		System.out.println(cus3.toString());
		
		Customer cus4 = new Customer("Elizabeth", 20.52, 104.54, 5);
		System.out.println(cus4.toString());		
		
		Customer cus5 = new Customer("Jasdir", 476, 1000.96, 5);
		System.out.println(cus5.toString());	
}
}