package com.kiersten.test;

public class Student {
protected int n1;
protected int n2;
	

	public int getN1() {
		return n1;
	}

	public void setN1(int n1) {
		this.n1 = n1;
	}

	public int getN2() {
		return n2;
	}

	public void setN2(int n2) {
		this.n2 = n2;
	}
	
	protected int getAddition(int n1, int n2) {
		int sum = n1 + n2;
		return sum;
	}
}