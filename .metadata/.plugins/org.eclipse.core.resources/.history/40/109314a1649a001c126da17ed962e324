package com.revature.java.store;

public class Customer {
	private String name;
	private double purchaseAmt;
	public final int storeID = 4162;
	public static String location = "Cleveland, Georgia";
	
	
	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public double getPurchaseAmt() {
		return purchaseAmt;
	}

	public void setPurchaseAmt(double purchaseAmt) {
		this.purchaseAmt = purchaseAmt;
	}

	public Customer() {
	}

	public Customer(String name, double d) {
		super();
		this.name = name;
		this.purchaseAmt = d;
	}
	
	@Override
	public String toString() {
		return "Customer [name=" + name + ", purchaseAmt=" + purchaseAmt + "]";
	}
}
	


